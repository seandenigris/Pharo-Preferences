{ #category : #accessing }
PharoCorePackages class >> nonCorePackages [
	"SystemVersion current"
	| systemPackages |
	systemPackages := self for8 sorted.
	^ RPackage organizer packages reject: [ :e | systemPackages includes: e name ]
]

{ #category : #accessing }
PharoCorePackages class >> recordCurrent [
	"declare the list of currently loaded packages as Core for this software update"

	| savedSystemPackageNames updateNumber source sortedPackageNames ignoredPackagePatterns |
	updateNumber := SystemVersion current highestUpdate asString.
	ignoredPackagePatterns := #('DeNigris' 'MonticelloLogin' 'ActivePapers').
	savedSystemPackageNames := MCPackage allInstances asSet 
		select: [ :e | e workingCopy ancestors isEmpty not and: [ (ignoredPackagePatterns anySatisfy: [ :pattern | e name includesSubstring: pattern ]) not ] ] 
		thenCollect: [ :e | e workingCopy currentVersionInfo name ].
	sortedPackageNames := savedSystemPackageNames asOrderedCollection sort.
	source := String streamContents: [ :str |
		str
			nextPutAll: 'forUpdate';
			nextPutAll: updateNumber asString;
			cr;
			cr;
			tab;
			store: sortedPackageNames ].
	Halt now.
	self class compile: source classified: 'updates'.
]
